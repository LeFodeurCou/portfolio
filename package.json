{
  "name": "public",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "make-all-dir": "make-dir _dev/assets/ && make-dir _dev/scss/ && make-dir _dev/js/ && make-dir _dev/jsx/ && make-dir public",
    "del-not-usefull-dir": "del _dev/scss/ && del _dev/js/",
    "start": "parcel _dev/index.html --out-dir $npm_config_public --open",
    "mk": "make-dir $npm_config_public",
    "clean": "del --force $npm_config_public",
    "parcelDev": "parcel build _dev/index.html --out-dir $npm_config_public",
    "dev": "NODE_ENV=dev run-s clean mk parcelDev postRun",
    "parcelProd": "parcel build --no-source-maps _dev/index.html --out-dir $npm_config_public",
    "prod": "NODE_ENV=production run-s clean mk parcelProd postRun",
    "postRun": "ncp admin $npm_config_public/admin/ && ncp api $npm_config_public/api/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "del-cli": "^4.0.0",
    "make-dir-cli": "^3.0.0",
    "move-cli": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.3",
    "replace-in-files-cli": "^1.0.0",
    "sass": "^1.35.2"
  },
  "dependencies": {
    "ncp": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
